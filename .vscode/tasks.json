{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "processing_java",
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "command": "${config:processing.path}",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
            },
            "args": [
                "--force",
                "--sketch=${workspaceRoot}",
                "--output=${workspaceRoot}\\out",
                "--run"
            ]
        }
    ]
}